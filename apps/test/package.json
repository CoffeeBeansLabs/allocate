  "scripts": {
    "webScript": "wdio run ./build/**/web.wdio.conf.js",
    "iosScript": "wdio run ./build/**/ios.wdio.conf.js",
    "androidScript": "wdio run ./build/**/android.wdio.conf.js",
    "remoteAndroidScript": "wdio run ./build/**/remote.android.wdio.conf.js",
    "remoteWebScript": "wdio run ./build/**/remote.web.wdio.conf.js",
    "cleanDir": "rimraf ./allure-results/**",
    "generateAllureReport": "npx allure generate allure-results --clean",
    "openAllureReport": "npx allure open",
    "build": "tsc && npm run cleanDir",
    "web": "npm run build && npm run webScript",
    "android": "npm run build && npm run androidScript",
    "ios": "npm run build && npm run iosScript",
    "remoteAndroid": "npm run build && npm run remoteAndroidScript",
    "remoteWeb": "npm run build && npm run remoteWebScript"
  },
  "dependencies": {
    "15": "0.0.0",
    "@types/node": "^20.14.9",
    "googleapis": "^140.0.1"
