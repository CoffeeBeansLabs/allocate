    "@wdio/sauce-service": "^8.11.2",
    "@wdio/spec-reporter": "^8.11.2",
    "allure-commandline": "^2.22.4",
    "appium": "^1.22.3",
    "chai": "^4.3.7",
    "chromedriver": "^126.0.4",
    "clean-dir": "^1.0.3",
    "dotenv": "^16.4.5",
    "driver": "^1.0.0",
